{
  "name": "react-starter-project",
  "version": "0.0.0",
  "description": "Starter React project that uses webpack, react-router, and redux.",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --config webpack.config.dev.babel.js",
    "build:prod:server": "NODE_ENV=production babel src/server --out-dir build/dist",
    "build:prod:client": "NODE_ENV=production webpack --progress --colors --config webpack.config.prod.babel.js",
    "build:prod": "npm run build:prod:client && npm run build:prod:server",
    "postinstall": "npm run build:prod",
    "start": "node build/dist/server.js",
    "test": "jest --config jest-config.json && npm run lint",
    "lint:eslint": "eslint src test --ext .js,.jsx",
    "lint:stylelint": "stylelint src/**/*.scss",
    "lint": "npm run lint:eslint; npm run lint:stylelint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djtsai/react-starter-project.git"
  },
  "engines": {
    "node": "^12.11.1",
    "npm": "^6.11.3"
  },
  "author": "Daniel Tsai <danieltsai.developer@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.6",
    "helmet-csp": "^2.9.4",
    "is-port-reachable": "^3.0.0",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-webpack-plugin": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}
