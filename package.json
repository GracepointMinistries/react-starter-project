{
  "name": "react-starter-project",
  "version": "0.0.0",
  "description": "Starter React project that uses webpack, react-router, and redux.",
  "scripts": {
    "dev": "webpack serve --inline --hot --config webpack.config.dev.babel.js",
    "build:prod:server": "NODE_ENV=production babel src/server --out-dir build/dist",
    "build:prod:client": "NODE_ENV=production webpack --progress --color --config webpack.config.prod.babel.js",
    "build:prod": "npm run build:prod:client && npm run build:prod:server",
    "postinstall": "npm run build:prod",
    "start": "node build/dist/server.js",
    "test": "jest --config jest-config.json && npm run lint",
    "lint:eslint": "eslint src test --ext .js,.jsx",
    "lint:stylelint": "stylelint src/**/*.scss",
    "lint": "npm run lint:eslint; npm run lint:stylelint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djtsai/react-starter-project.git"
  },
  "engines": {
    "node": "^14.16.0",
    "npm": "^6.14.11"
  },
  "author": "Daniel Tsai <danieltsai.developer@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.1.1",
    "helmet-csp": "^3.3.1",
    "is-port-reachable": "^3.0.0",
    "morgan": "^1.10.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
